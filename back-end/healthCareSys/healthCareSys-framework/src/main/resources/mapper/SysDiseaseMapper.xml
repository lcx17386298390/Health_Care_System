<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.acm.mapper.DiseaseMapper">

    <resultMap id="BaseResultMap" type="com.acm.entity.Disease">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="pid" column="pid" jdbcType="VARCHAR"/>
            <result property="pname" column="pname" jdbcType="VARCHAR"/>
            <result property="diseaseName" column="disease_name" jdbcType="VARCHAR"/>
            <result property="clinicDate" column="clinic_date" jdbcType="DATE"/>
            <result property="did" column="did" jdbcType="VARCHAR"/>
            <result property="dname" column="dname" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,pid,pname,
        disease_name,clinic_date,did,
        dname
    </sql>
</mapper>
