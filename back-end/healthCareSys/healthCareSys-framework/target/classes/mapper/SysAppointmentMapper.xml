<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.acm.mapper.AppointmentMapper">

    <resultMap id="BaseResultMap" type="com.acm.entity.Appointment">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="did" column="did" jdbcType="VARCHAR"/>
            <result property="pid" column="pid" jdbcType="VARCHAR"/>
            <result property="appointmentDate" column="appointment_date" jdbcType="DATE"/>
            <result property="appointmentStatus" column="appointment_status" jdbcType="VARCHAR"/>
            <result property="doctorName" column="doctor_name" jdbcType="VARCHAR"/>
            <result property="patientName" column="patient_name" jdbcType="VARCHAR"/>
            <result property="appointmentDepartment" column="appointment_department" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,did,pid,
        appointment_date,appointment_status,doctor_name,
        patient_name,appointment_department
    </sql>
</mapper>
