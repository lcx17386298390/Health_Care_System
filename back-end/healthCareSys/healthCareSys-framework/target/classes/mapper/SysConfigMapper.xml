        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.acm.mapper.">

    <resultMap id="BaseResultMap" type="resources.mapper.SysConfig">
            <id property="variable" column="variable" jdbcType="VARCHAR"/>
